<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.itheima.spring.domain.SaleVisit" table="sale_visit">
		<id name="visit_id">
			<generator class="uuid" />
		</id>
		<property name="visit_time" />
		<property name="visit_addr" />
		<property name="visit_detail" />
		<property name="visit_nexttime" />
		
		<many-to-one name="customer" class="com.itheima.spring.domain.Customer" column="visit_cust_id"></many-to-one>
		<many-to-one name="user" class="com.itheima.spring.domain.User" column="visit_user_id"></many-to-one>
		
	</class>

</hibernate-mapping>